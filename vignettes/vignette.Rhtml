<pre class="src src-r">org_clock_df(orgfile) %&gt;%
    filter(between(Date, Sys.Date() - 60, Sys.Date())) %&gt;%
    group_by(Date, Headline) %&gt;%
    summarise(TimeSpent) %&gt;%
    ungroup() %&gt;%
    arrange(desc(TimeSpent))
