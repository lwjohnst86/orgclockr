<pre class="src src-r">org_clock_df(orgfile) %&gt;%
    group_by(Headline) %&gt;%
    summarise(AvgTimeOnTask = round(sum(TimeSpent)/sum(NIntervals), 2)) %&gt;%
    arrange(desc(AvgTimeOnTask))
</pre>
